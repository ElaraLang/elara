module Prelude
import Elara.Prim
import List

#LeftAssociative #Fixity 0
def (|>) : a -> (a -> b) -> b
let (|>) x f = f x

-- Right associative function composition
def (>>) : (a -> b) -> (b -> c) -> (a -> c)
let (>>) f g = \x -> g (f x)

#RightAssociative #Fixity 1
def (*>) : IO a -> IO b -> IO b
let (*>) m1 m2 = m1 >>= (\_ -> m2)