import Prelude 
import Elara.Prim

let part1 input = 
    let chars = input |> stringToList |> map charToInt |> filter (\x -> x /= -1)
    match chars with
        [] -> 0
        (first::xs) ->
            let last = last chars
            (first * 10) + last

let main = 
    let contents = readFile "../input.txt"
    contents `ioBind` (\contents -> contents |> map part1 |> sum |> print)