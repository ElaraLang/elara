import Prelude 
import Elara.Prim

def fact : Int -> Int
let fact n = 
    if n == 0 then 1 else n * fact (n - 1)

let part1 input = 
    let chars = filter (\x -> x /= -1) (stringToList input |> (map charToInt))
    let first = head chars
    let last = last chars
    first * 10 + last

let main = 

    print (part1 "1abc2")
