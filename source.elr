import Prelude 
import Elara.Prim

let part1 input = 
    let chars = input |> stringToList |> map charToInt |> filter (\x -> x /= -1)
    match chars with
        [] -> 0
        (first::forTechnicalReasonsThisCantBeAWildcard) ->
            let last = last chars
            (first * 10) + last
def run : [String] -> Int
let run = map part1 >> sum

let main = 
    let contents = readFile "../input.txt"
    contents >>= (run >> print)
