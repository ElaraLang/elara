module Prelude
import Elara.Prim

def print : a -> IO ()
let print a = println (toString a)

def (++) : [a] -> [a] -> [a]
let (++) a b = 
    match a with
      [] -> b
      (x::xs) -> x :: (xs ++ b)

def reverse : [a] -> [a]
let reverse a = 
    match a with
      [] -> []
      (x::xs) -> reverse xs ++ [x]

def map : (a -> b) -> [a] -> [b]
let map f a = 
    match a with
      [] -> []
      (x::xs) -> f x :: map f xs
